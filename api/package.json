{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "node ace build --production",
    "start": "node server.js",
    "dev": "node ace serve --watch",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "check-prettier": "prettier-check \"**/*.{js,ts}\"",
    "check-lint": "eslint \"**/*.{js,ts}\" --max-warnings 0 ",
    "checks": "yarn run check-lint && yarn run check-prettier"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^3.0.0",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-adonis": "^1.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "pino-pretty": "^4.7.1",
    "prettier": "^2.2.1",
    "prettier-check": "^2.0.0",
    "typescript": "~4.1",
    "youch": "^2.2.1",
    "youch-terminal": "^1.1.0"
  },
  "dependencies": {
    "@adonisjs/core": "~5.0.4-preview-rc",
    "@adonisjs/repl": "^1.0.0",
    "@aws-sdk/client-sns": "^3.11.0",
    "firebase-admin": "^9.6.0",
    "firestore-storage": "^4.3.1",
    "proxy-addr": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19"
  }
}
